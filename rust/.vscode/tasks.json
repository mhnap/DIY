// Not using <https://github.com/cargo-limit/cargo-limit>.
{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "cargo check",
			"type": "cargo",
			"command": "check",
			"group": {
				"kind": "build",
				"isDefault": true
			},
		},
		{
			"label": "cargo build",
			"type": "cargo",
			"command": "build",
			"group": {
				"kind": "build"
			},
		},
		{
			"label": "cargo clippy",
			"type": "cargo",
			"command": "clippy",
			"group": {
				"kind": "build"
			},
		},
		{
			"label": "cargo fmt",
			"type": "cargo",
			"command": "fmt",
			"group": {
				"kind": "build"
			},
		},
	]
}